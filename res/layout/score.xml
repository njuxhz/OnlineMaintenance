<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
    <Button   
            android:id="@+id/scoreretBT"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="返回" />
    <LinearLayout 
    	    android:layout_width="match_parent"
    		android:layout_height="wrap_content"
    		android:orientation="horizontal" 
    		android:gravity="center">
    	    <Button   
            	android:id="@+id/m1BT"
            	android:layout_width="50sp"
            	android:layout_height="wrap_content"
            	android:text="1" />
    	    <Button   
            	android:id="@+id/m2BT"
            	android:layout_width="50sp"
            	android:layout_height="wrap_content"
            	android:text="2" />
    	    <Button   
            	android:id="@+id/m3BT"
            	android:layout_width="50sp"
            	android:layout_height="wrap_content"
            	android:text="3" />
    	    <Button   
            	android:id="@+id/m4BT"
            	android:layout_width="50sp"
            	android:layout_height="wrap_content"
            	android:text="4" />
    	    <Button   
            	android:id="@+id/m5BT"
            	android:layout_width="50sp"
            	android:layout_height="wrap_content"
            	android:text="5" />
    	    <Button   
            	android:id="@+id/m6BT"
            	android:layout_width="50sp"
            	android:layout_height="wrap_content"
            	android:text="6" />
    	</LinearLayout>
	    <LinearLayout 
    	    android:layout_width="match_parent"
    		android:layout_height="wrap_content"
    		android:orientation="horizontal" 
    		android:gravity="center">
    	    <Button   
            	android:id="@+id/m7BT"
            	android:layout_width="50sp"
            	android:layout_height="wrap_content"
            	android:text="7" />
    	    <Button   
            	android:id="@+id/m8BT"
            	android:layout_width="50sp"
            	android:layout_height="wrap_content"
            	android:text="8" />
    	    <Button   
            	android:id="@+id/m9BT"
            	android:layout_width="50sp"
            	android:layout_height="wrap_content"
            	android:text="9" />
    	    <Button   
            	android:id="@+id/m10BT"
            	android:layout_width="50sp"
            	android:layout_height="wrap_content"
            	android:text="10" />
    	    <Button   
            	android:id="@+id/m11BT"
            	android:layout_width="50sp"
            	android:layout_height="wrap_content"
            	android:text="11" />
    	    <Button   
            	android:id="@+id/m12BT"
            	android:layout_width="50sp"
            	android:layout_height="wrap_content"
            	android:text="12" />
    	</LinearLayout>
    	<TableLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:stretchColumns="1" >
    	<TableRow
            android:layout_width="wrap_content"
            android:layout_height="35sp" >
            <TextView
                android:layout_height="35sp"
                android:paddingLeft="30dp"
                android:text="当前选择月份"
                android:typeface="serif"
                android:textSize="15sp" />
            <EditText
                android:id="@+id/selectmonthET"
                android:paddingLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="35sp" />
        </TableRow>
	    <TableRow
            android:layout_width="wrap_content"
            android:layout_height="35sp" >
            <TextView
                android:layout_height="35sp"
                android:paddingLeft="30dp"
                android:text="总分"
                android:typeface="serif"
                android:textSize="15sp" />
            <EditText
                android:id="@+id/totalscoreET"
                android:paddingLeft="10dp"
                android:layout_width="wrap_content"
                android:layout_height="35sp" />
        </TableRow>
    </TableLayout>
    	<ListView 
	    android:id="@+id/user_list_view"
	    android:layout_width="match_parent"
	    android:layout_height="match_parent" >
	</ListView>
</LinearLayout>
